### config
# if not set to true, will expose service ports to the host. If true, will expect the reverse proxy to route to services.
PROD=false
# if set to true, will configure the reverse proxy to allow insecure connections via service ports, otherwise will setup https
INSECURE=false
ES_HEAP_SIZE=-Xms4096m -Xmx4096m
# the domain name to use for setting up certs and accessing the server, can be an IP address if using INSECURE mode
DOMAIN_NAME=localhost
# The email address to use for letsencrypt renewal notifications
RENEWAL_EMAIL=test@test.com

