{
  "name": "cdr-tests",
  "version": "0.1.0",
  "main": "index.js",
  "author": "Jembi",
  "license": "MIT",
  "dependencies": {
    "concurrently": "^6.0.2",
    "newman": "^5.2.2"
  },
  "scripts": {
    "bootstrap": "yarn newman run 'CDR.postman_collection.json' --folder 'Bootstrap'",
    "test": "yarn newman run 'CDR.postman_collection.json' --folder 'Submit random bundle' --folder 'Submit random CSV'",
    "test:1000": "yarn test -n 1000",
    "test:csv": "yarn newman run 'CDR.postman_collection.json' --folder 'Submit random CSV'",
    "test:csv:1000": "yarn test:csv -n 1000",
    "test:bundle": "yarn newman run 'CDR.postman_collection.json' --folder 'Submit random bundle'",
    "test:bundle:1000": "yarn test:bundle -n 1000",
    "bootstrap:qa": "yarn bootstrap --env-var openhimHost=qa.disi-cdr.jembi.org",
    "test:qa": "yarn test --env-var openhimHost=qa.disi-cdr.jembi.org",
    "test:qa:1000": "yarn test:1000 --env-var openhimHost=qa.disi-cdr.jembi.org",
    "bootstrap:stg": "yarn bootstrap --env-var openhimHost=stg.disi-cdr.jembi.org",
    "test:stg": "yarn test --env-var openhimHost=stg.disi-cdr.jembi.org",
    "test:stg:1000": "yarn test:1000 --env-var openhimHost=stg.disi-cdr.jembi.org"
  }
}
